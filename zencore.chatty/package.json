{
  "name": "zen-core-chatty-ts",
  "version": "1.0.0",
  "description": "ZenCore Chatty TypeScript SDK : for all ZenChatties",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "prebuild": "npm run build:types",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "chat",
    "messaging",
    "typescript",
    "sdk",
    "api",
    "axios"
  ],
  "author": "fr1g",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/zen-core-chatty-typescript.git"
  },
  "homepage": "https://github.com/your-username/zen-core-chatty-typescript",
  "bugs": {
    "url": "https://github.com/your-username/zen-core-chatty-typescript/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models": {
      "import": "./dist/models/index.js",
      "types": "./dist/models/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./signalr-client": {
      "import": "./dist/signalr-client.js",
      "types": "./dist/signalr-client.d.ts"
    },
    "./tools": {
      "import": "./dist/tools/index.js",
      "types": "./dist/tools/index.d.ts"
    }
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@microsoft/signalr": "^10.0.0",
    "@reduxjs/toolkit": "^2.11.0",
    "axios": "^1.13.2",
    "redux": "^5.0.1"
  }
}